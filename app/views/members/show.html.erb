<html>
<% if notice %>
  <p id="notice"><%= notice %></p>
<% end %>

<h2>Member Page</h2>
<ul>
  <% @member.enrollments.each do |item| %>
    <li><%= item.activity.name %>  <%= button_to('UnEnroll', enrollment_path(id: item), :method => :delete,
      data: { confirm: 'Are you sure?' } )%></li>
  <% end %>
</ul>
</html>
